# 🔐 환경변수 설정 파일
# 
# 이 파일을 .env 로 복사하여 실제 값들로 설정하세요
# .env 파일은 Git에 커밋되지 않으므로 안전합니다

# ============================================
# Google Sheets 설정
# ============================================

# Google Sheets ID (스프레드시트 URL에서 추출)
# 예: https://docs.google.com/spreadsheets/d/1ABC123DEF456GHI789/edit
# → 1ABC123DEF456GHI789 부분만 복사
VITE_GOOGLE_SHEET_ID=YOUR_SHEET_ID_HERE

# ============================================
# Google Forms 설정 (나중에 설정 예정)
# ============================================

# 신규 추가용 Google Form URL
VITE_GOOGLE_FORM_ADD_URL=

# 수정용 Google Form URL  
VITE_GOOGLE_FORM_EDIT_URL=

# 삭제 요청용 Google Form URL
VITE_GOOGLE_FORM_DELETE_URL=

# ============================================
# 기타 설정
# ============================================

# 데이터 새로고침 간격 (밀리초, 기본값: 30초)
VITE_DATA_REFRESH_INTERVAL=30000

# 개발 모드에서 더미 데이터 사용 여부
VITE_USE_DUMMY_DATA=false
